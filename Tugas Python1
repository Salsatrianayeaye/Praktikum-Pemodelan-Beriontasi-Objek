# Praktikum-Pemodelan-Beriontasi-Objek
Tugas Praktikum Pemodelan Berontasi Objek
kalimat = "ini adalah kalimat"
paragraf = """ini adalah paragraf.
Paragraf terdiri dari beberapa kalimat."""
print(kalimat)
print(paragraf)
ini adalah kalimat
ini adalah paragraf.
Paragraf terdiri dari beberapa kalimat.


kalimat = "abcdefghijklmnopqrstuvwxyz"
print(kalimat[:5])
print(kalimat[5:10])
print(kalimat[21:])
print(kalimat[-10:-1])
print(kalimat[-10:])
abcde
fghij
vwxyz
qrstuvwxy

kalimat = "Halo, semua. Saya adalah programer Python."
print(kalimat.upper())
print(kalimat.lower())
print(kalimat.capitalize())
print(kalimat.title())
print(kalimat.swapcase())
print(kalimat.replace("Python", "Java"))
print(kalimat.replace("a", "i"))
print(kalimat.strip())
print(kalimat.split('.'))
HALO, SEMUA. SAYA ADALAH PROGRAMER PYTHON.
halo, semua. saya adalah programer python.
Halo, semua. saya adalah programer python.
Halo, Semua. Saya Adalah Programer Python.
hALO, SEMUA. sAYA ADALAH PROGRAMER pYTHON.
Halo, semua. Saya adalah programer Java.
Hilo, semui. Siyi idilih progrimer Python.
Halo, semua. Saya adalah programer Python.

kalimat = "Halo, semua. Saya adalah programe Python."
tambahan = "Saya sedang belajar Python. Mohon bimbingannya"
